            <div class="sign_up">Sign up for free</div>
          </div>
          <!--left text start-->
          <!--video  start-->
          <div class="video_con">
            <div id="banner">
              <div class="oneByOne_item"> <img src="images/Slide41.jpg"  class="bigImage" data-animate="bounceIn"></img> </div>
              <div class="oneByOne_item"> <img src="images/Slide3.jpg"  class="bigImage"></img> </div>
              <div class="oneByOne_item"> <img src="images/Slide2.jpg"  class="bigImage" ></img> </div>
              <div class="oneByOne_item"> <img src="images/Slide1.jpg"  class="bigImage" ></img> </div>
            </div>
          </div>
          <!--video  start-->
        </div>
        <!--video left text end-->
        <div class="take_tour_btn">Take a tour</div>
      </div>
      <div class="bottom_background"></div>
    </div>
    <!--main text end-->
  </div>
  <!--middle end-->
  <!--footer start-->
  <div class="wrapper_2">
    <div class="footer_con">
      <!--footer menu links start-->
      <div class="footer_link">
        <!--footer link navigation start-->

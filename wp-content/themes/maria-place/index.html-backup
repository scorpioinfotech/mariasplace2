            <div class="name_link">HI Ryan</div>
            <div class="drop_arrow">
              <svg class="svg-triangle" fill="white">
                <polygon points="0,0 18,0 10,13"/>
              </svg>
            </div>
          </div>
          <div class="ryan_image"><img src="images/ryan_image.png" alt="image" /> </div>
        </div>
      </div>
      <!---navigation ends-->
    </div>
    <!--header wrapper end-->
  </header>
  <!--header end-->
